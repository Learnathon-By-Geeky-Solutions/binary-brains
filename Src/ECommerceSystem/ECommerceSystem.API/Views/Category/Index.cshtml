@model List<Category>

	<h1>Category List</h1>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Category Name</th>
            <th>Display Order</th>
            <th>Created By</th>
            <th>Created Date</th>
        </tr>
	</thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.DsiplayOrder</td>
                <td>@obj.CreatedBy</td>
                <td>@obj.CreatedDate</td>
            </tr>
        }

    </tbody>

</table>